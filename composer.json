{
    "name": "madkom/pact-php-client",
    "description": "Client written in PHP for Pact-Mock-Service",
    "type": "library",
    "prefer-stable": true,
    "license": "MIT",
    "homepage": "http://madkom.pl/",
    "keywords": [
        "pact",
        "mock",
        "service",
        "consumer",
        "driven",
        "contract",
        "client",
        "madkom"
    ],
    "authors": [
        {
            "name": "Darek Gafka",
            "email": "d.gafka@madkom.pl"
        }
    ],
    "require": {
        "php": "^5.6",
        "psr/http-message": "^1.0",
        "php-http/httplug": "^1.0.0",
        "guzzlehttp/psr7": "^1.2.2",
        "symfony/process": "^3.4"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4.1",
        "phpunit/phpunit": "^5.2.4",
        "henrikbjorn/phpspec-code-coverage": "^2.1",
        "phpunit/php-code-coverage": "^3.0",
        "phpunit/phpcov": "^3.0.0",
        "satooshi/php-coveralls": "^1.0",
        "php-http/guzzle6-adapter": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Madkom\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
